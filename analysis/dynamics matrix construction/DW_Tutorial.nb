Cell[TextData[{
 StyleBox["BodyToJnt->0 | ",
  FontFamily->"Courier",
  FontSize->12,
  FontWeight->"Bold"],
 StyleBox["bodytojoint",
  FontFamily->"Courier",
  FontSize->12,
