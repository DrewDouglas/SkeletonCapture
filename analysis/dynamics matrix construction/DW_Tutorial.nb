Cell[TextData[{
 "To set the initial conditions, write equations for each of the states (q's \
and u's), and apply ",
 StyleBox["AtT0",
  FontFamily->"Courier",
  FontSize->12,
  FontWeight->"Bold"],
 " to specify that the equations hold for time zero."
}], "Text",
 Evaluatable->False,
 AspectRatioFixed->True],

Cell[BoxData[
 RowBox[{
  RowBox[{"initcond", "=", 
   RowBox[{"AtT0", "[", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{"q", "[", "1", "]"}], "==", 
       RowBox[{"14", " ", "Degrees"}]}], ",", 
      RowBox[{
       RowBox[{"u", "[", "1", "]"}], "==", 
       RowBox[{
        RowBox[{"-", "80"}], " ", "Degrees"}]}], ",", 
      RowBox[{
       RowBox[{"q", "[", "2", "]"}], "==", 
       RowBox[{
        RowBox[{"-", "14"}], " ", "Degrees"}]}], ",", 
      RowBox[{
       RowBox[{"u", "[", "2", "]"}], "\[Equal]", 
       RowBox[{"90", " ", "Degrees"}]}], ",", 
      RowBox[{
       RowBox[{"q", "[", "3", "]"}], "==", 
       RowBox[{
        RowBox[{"-", "60"}], " ", "Degrees"}]}], ",", 
      RowBox[{
       RowBox[{"u", "[", "3", "]"}], "==", 
       RowBox[{
        RowBox[{"-", "10"}], " ", "Degrees"}]}]}], "}"}], "]"}]}], 
  ";"}]], "Input",
 PageBreakWithin->Automatic,
 GroupPageBreakWithin->Automatic,
 AspectRatioFixed->True],

Cell[TextData[{
 "The trajectory can be generated by solving the set of differential \
equations including the equations of motion, the kinematical differential \
equations (which are generated automatically by the ",
 StyleBox["Dynamics Workbench",
  FontWeight->"Bold"],
 "), subject to the initial conditions. Here we integrate from time 0 to 0.5 \
seconds."
}], "Text",
 Evaluatable->False,
 AspectRatioFixed->True],

Cell[BoxData[
 RowBox[{
  RowBox[{"trajectory", "=", 
   RowBox[{"NDSolve", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"Join", "[", 
       RowBox[{"eom", ",", "Kinematics", ",", "initcond"}], "]"}], "//.", 
      "constants"}], ",", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"q", "[", "1", "]"}], ",", 
       RowBox[{"u", "[", "1", "]"}], ",", 
       RowBox[{"q", "[", "2", "]"}], ",", 
       RowBox[{"u", "[", "2", "]"}], ",", 
       RowBox[{"q", "[", "3", "]"}], ",", 
       RowBox[{"u", "[", "3", "]"}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"t", ",", "0.", ",", "0.5"}], "}"}]}], "]"}]}], ";"}]], "Input",\

 PageBreakWithin->Automatic,
 GroupPageBreakWithin->Automatic,
 AspectRatioFixed->True],

Cell["\<\
This plot shows how knee velocity approaches zero just before stance and \
swing legs reach the same angle for heel strike. The solid  line is the \
stance leg angle, the dashed line is the swing leg thigh angle, and the \
dotted line is the angle between the swing thigh and shank (also known as \
knee angle).\
\>", "Text",
 Evaluatable->False,
 AspectRatioFixed->True],

